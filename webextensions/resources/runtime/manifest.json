{
  "name": "Web Platform Tests - browser.runtime tests",
  "update_url": "https://web-platform.test/webext-update",
  "version": "0.1",
  "manifest_version": 3,
  "description": "tests for browser.runtime",
  "key":"",
  "content_scripts": [
    {
      "matches": [
        "http://web-platform.test/webextensions/browser.runtime.html"
      ],
      "js": [
        "util.js",
        "tests/prototype.js",
        "tests/connect.js",
        "tests/ContextType.js",
        "tests/getBrowserInfo.js",
        "tests/getFrameId.js",
        "tests/getManifest.js",
        "tests/getPlatformInfo.js",
        "tests/getPackageDirectoryEntry.js",
        "tests/getURL.js",
        "tests/id.js"
      ]
    },{
      "matches": [
        "http://web-platform.test:8000/webextensions/browser.runtime.html"
      ],
      "js": [
        "util.js",
        "tests/prototype.js",
        "tests/connect.js",
        "tests/ContextType.js",
        "tests/getBrowserInfo.js",
        "tests/getFrameId.js",
        "tests/getManifest.js",
        "tests/getPlatformInfo.js",
        "tests/getPackageDirectoryEntry.js",
        "tests/getURL.js",
        "tests/id.js"
      ]
    }
  ],
  "icons": {
    "512": "icon.jpg"
  },
  "web_accessible_resources": [
    {
      "resources": [ "utility_page.html" ],
      "matches": [
        "http://web-platform.test/*"
      ]
    }
  ],
  "host_permissions": [
    "http://web-platform.test/*"
  ],
  "nonStandardProp": "nonStandardValue"
}
